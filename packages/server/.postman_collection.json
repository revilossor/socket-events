		{
			"name": "add an \"init\" event with no data to aggregateId \"integration\"",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "1c066162-3933-4c60-b6bd-b68ed88aa711",
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"pm.test(\"Response should have aggregateId\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.aggregateId).to.eql(\"integration\");",
							"});",
							"pm.test(\"Response should have body\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.body).to.eql({ event: \"init\" });",
							"});",
							"pm.test(\"Response should have version 0\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.version).to.eql(0);",
							"});"
